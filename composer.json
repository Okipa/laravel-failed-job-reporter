{
    "name": "okipa/laravel-failed-jobs-notifier",
    "description": "Get notified when failed jobs are stuck for a defined number of days.",
    "keywords": [
        "okipa",
        "package",
        "php",
        "laravel",
        "queue",
        "failed",
        "job",
        "jobs",
        "notify",
        "notifier",
        "laravel-failed-jobs-notifier"
    ],
    "homepage": "https://github.com/Okipa/laravel-failed-jobs-notifier",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur LORENT",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.5.0||~5.6.0||~5.7.0||~5.8.0||^6.0",
        "laravel-notification-channels/webhook": "^1.3",
        "laravel/slack-notification-channel": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "nunomaduro/larastan": "^0.4",
        "orchestra/testbench": "~3.8.0||^4.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpmd/phpmd": "^2.7",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Okipa\\LaravelFailedJobsNotifier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okipa\\LaravelRequestSanitizer\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd config,src text phpmd.xml",
            "vendor/bin/phpstan analyse",
            "vendor/bin/phpunit"
        ]
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows notifications to be sent via Slack"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okipa\\LaravelFailedJobsNotifier\\FailedJobsNotifierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
